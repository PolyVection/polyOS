auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp

auto wlan0
iface wlan0 inet dhcp
  wireless_mode managed
  wireless_essid any
  wpa-driver wext
  wpa-conf /mnt/data/settings/wireless/wifi.conf

